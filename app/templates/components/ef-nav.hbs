{{#if hasBlockParams}}
  {{#ef-nav/nav-list
    inline=inline
    pills=pills
  }}
    {{yield (hash
      divider=(component
        "ef-nav/nav-list-divider"
      )

      group-header=(component
        "ef-nav/nav-list-group-header"
        label=label
      )

      header=(component
        "ef-nav/nav-list-header"
        label=label
      )

      link=(component
        "ef-nav/nav-list-item"
        active=active
        disabled=disabled
        label=label
        onClick=onClick
      )
    )}}
  {{/ef-nav/nav-list}}
{{else}}
  You must use block params (<code>as | |</code>) when using the <code>ef-nav</code> component.
{{/if}}
